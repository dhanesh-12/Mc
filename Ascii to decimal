ORG 0

MOV R5, #"4" // LOADING R5 WITH ASCII EQUIVALENT OF 4 (IE, 34)

MOV R6, #"7" // LOADING R6 WITH ASCII EQUIVALENT OF 7 (IE, 37)

MOV A, R5

ANL A, #0FH // MASK LOWER NIBBLE

MOV R5, A

MOV A, R6

ANL A, #0FH // MASK UPPER NIBBLE

MOV R6, A

MOV A, R5

RL A

RL A

RL A

RL A

ORL A, R6 // PACKING

MOV P1,A 

SJMP $

END
